{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "singleAttributePerLine": true,
  "arrayExpand": false,
  "singleQuote": false
}
